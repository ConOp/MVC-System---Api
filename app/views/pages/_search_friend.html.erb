<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title" id="exampleModalLabel">Add a Friend</h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <!--<input type="text" name="" value="" placeholder="example@email.com">-->
        <%= form_tag search_path, :method => :post, :id => "SearchForm",remote: true do %>
        <%= text_field_tag :search, params[:search], placeholder: 'example@email.com',  :id => 'SearchSearch'  %>
        <%= submit_tag('Search') %>
        <% end %>
        <div id='userslist'>
        </div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
